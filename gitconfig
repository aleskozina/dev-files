[core]
	editor = nano
	excludesfile = ~/.gitignore
  autocrlf = input
  eol = lf
[alias]
    f = fetch
    fa = fetch --all
    s = status
    c = commit -m
    d = diff
    pl = pull
    ps = push
    pom = push origin master
    ac = !git add --all && git commit -m
[color]
    ui = true
[push]
    default = current
